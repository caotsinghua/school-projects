.pingjia{
  width: 100%;
  height: 100vh;
  background: #ccc;
}
.table-head{
  width: 100%;
  display: flex;
  padding: 10rpx 20rpx;
  border-bottom: 1px solid #eee;
}
.table-head .col1{
  width: 550rpx;
  border-right: 1px solid #eee;
  text-align: center;
}
.table-head .col2{
  text-align: center;
}

.member{
  display: flex;
  border-bottom: 1px solid #eee;
  padding: 20rpx;
  align-items: center;
}
.member .userinfo{
  display: flex;
  width: 550rpx;
  align-items: center;
}
.member .userinfo .avatar{
  width:90rpx;
  height:90rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.member-wrap{
  margin-bottom: 30rpx;
}